<Window x:Class="GUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="qap-project" Height="350" Width="525">
    <DockPanel>
        <Button Name="loadData" Content="Wczytaj dane" DockPanel.Dock="Top"/>
        <Button Name="run" Content="Uruchom" DockPanel.Dock="Bottom"/>
        <StackPanel>
            <Label Name="loadedData" Content="Nie wczytano danych"/>
            <TabControl>
                <TabItem Header="Mrówkowy">
                    <Label Content="Tu będą parametry algorytmu mrówkowego"/>
                </TabItem>
                <TabItem Header="Pszczeli">
                    <Label Content="Tu będą parametry algorytmu pszczelego"/>
                </TabItem>
                <TabItem Header="Świetlikowy">
                    <Label Content="Tu będą parametry algorytmu świetlikowego"/>
                </TabItem>
            </TabControl>
        </StackPanel>
    </DockPanel>
</Window>
